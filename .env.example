# AI Face Swap Environment Configuration Example
# Copy this file to .env and update the values

# Application Settings
VITE_APP_NAME=AI Face Swap
VITE_APP_VERSION=1.0.0

# API Configuration
VITE_API_BASE_URL=http://localhost:3000
VITE_API_TIMEOUT=30000

# TensorFlow.js Configuration
VITE_TF_BACKEND=webgl
VITE_MAX_IMAGE_SIZE=4096
VITE_MODEL_CACHE_SIZE=100

# Feature Flags
VITE_ENABLE_DEBUG=false
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=true

# Image Processing
VITE_MAX_FILE_SIZE_MB=10
VITE_SUPPORTED_FORMATS=image/jpeg,image/png,image/webp
VITE_COMPRESSION_QUALITY=0.9

# Performance Settings
VITE_PROCESSING_TIMEOUT=60000
VITE_ENABLE_WEBGL2=true

# Third-party Services
VITE_ANALYTICS_ID=your_analytics_id_here
VITE_ERROR_TRACKING_ID=your_error_tracking_id_here

# Proxy Server
VITE_PROXY_SERVER_ACCESS_TOKEN=your_proxy_token_here
