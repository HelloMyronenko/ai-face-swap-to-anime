# AI Face Swap Environment Configuration

# Application Settings
VITE_APP_NAME=AI Face Swap
VITE_APP_VERSION=1.0.0

# API Configuration
VITE_API_BASE_URL=http://localhost:3000
VITE_API_TIMEOUT=30000

# TensorFlow.js Configuration
VITE_TF_BACKEND=webgl
VITE_MAX_IMAGE_SIZE=4096
VITE_MODEL_CACHE_SIZE=100

# Feature Flags
VITE_ENABLE_DEBUG=false
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=true

# Image Processing
VITE_MAX_FILE_SIZE_MB=10
VITE_SUPPORTED_FORMATS=image/jpeg,image/png,image/webp
VITE_COMPRESSION_QUALITY=0.9

# Performance Settings
VITE_PROCESSING_TIMEOUT=60000
VITE_ENABLE_WEBGL2=true

# Third-party Services (if needed in future)
VITE_ANALYTICS_ID=
VITE_ERROR_TRACKING_ID=

# Proxy Server (for future API integrations)
VITE_PROXY_SERVER_ACCESS_TOKEN=undefined

